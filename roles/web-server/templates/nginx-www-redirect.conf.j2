# {{ansible_managed}}

server {
  listen      *:80;
  server_name erincall.com;
  add_header X-Clacks-Overhead "GNU Terry Pratchett";

  return 301 https://www.erincall.com$request_uri;
}

server {
  server_name andrewlorente.com;
  listen      *:80;
  add_header X-Clacks-Overhead "GNU Terry Pratchett";

  return 301 https://www.erincall.com$request_uri;
}

server {
  listen      *:443 ssl;
  server_name erincall.com;
  add_header X-Clacks-Overhead "GNU Terry Pratchett";

  ssl on;
  ssl_certificate /etc/ssl/www_erincall_com.chained.crt;
  ssl_certificate_key /etc/ssl/www_erincall_com.key;
  ssl_session_cache shared:SSL:50m;
  ssl_session_timeout 5m;

  ssl_protocols TLSv1 TLSv1.2;
  ssl_ciphers "EECDH+ECDSA+AESGCM EECDH+aRSA+AESGCM EECDH+ECDSA+SHA384 EECDH+ECDSA+SHA256 EECDH+aRSA+SHA384 EECDH+aRSA+SHA256 EECDH EDH+aRSA !RC4 !aNULL !eNULL !LOW !3DES !MD5 !EXP !PSK !SRP !DSS";
  ssl_prefer_server_ciphers on;

  return 301 https://www.erincall.com$request_uri;
}

server {
  server_name  andrewlorente.com;
  listen      *:443 ssl;
  add_header X-Clacks-Overhead "GNU Terry Pratchett";

  ssl on;
  ssl_certificate /etc/ssl/www_andrewlorente_com.chained.crt;
  ssl_certificate_key /etc/ssl/www_andrewlorente_com.key;
  ssl_session_cache shared:SSL:50m;
  ssl_session_timeout 5m;

  ssl_protocols TLSv1 TLSv1.2;
  ssl_ciphers "EECDH+ECDSA+AESGCM EECDH+aRSA+AESGCM EECDH+ECDSA+SHA384 EECDH+ECDSA+SHA256 EECDH+aRSA+SHA384 EECDH+aRSA+SHA256 EECDH EDH+aRSA !RC4 !aNULL !eNULL !LOW !3DES !MD5 !EXP !PSK !SRP !DSS";
  ssl_prefer_server_ciphers on;
  return 301 https://www.erincall.com;
}
